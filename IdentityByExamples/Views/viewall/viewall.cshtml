@model IEnumerable<IdentityByExamples.Models.Transaction>


@{
    ViewData["Title"] = "Index";
}
<div class="row">
    <div class="col-md-8 offset-md-2">
       
       
         @foreach (var item in Model)
         {

            <h2>YOUR PROPERTY</h2>

            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">USER LISTING</h5>
                    <p class="card-text"> @Html.DisplayFor(modelItem => item.RENT)</p>
                    <p class="card-text"> @Html.DisplayFor(modelItem => item.ADDRESS)</p>
                    <p class="card-text"> @Html.DisplayFor(modelItem => item.ROOM)</p>
                    <p class="card-text"> @Html.DisplayFor(modelItem => item.BATHROOM)</p>
                </div>
            </div>
         }
     </div>

 </div>